{
    "name": "sam-burns/pimple-with-autowiring",

    "type": "library",

    "description": "A library for using an existing Pimple container alongside an autowiring container",

    "license": "MIT",

    "require": {
        "php": "^7.1",
        "pimple/pimple": "^3.2",
        "symfony/dependency-injection": "^4.1",
        "ultra-lite/composite-container": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "SamBurns\\PimpleWithAutoloading\\": "src/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1",
        "behat/behat": "^3.5",
        "phpunit/phpunit": "^7.4"
    }
}
